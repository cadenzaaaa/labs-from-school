#include <iostream>
using namespace std;

int main() {

   string input;
      
   while (getline(cin, input)) {
      
      if (input == "Done" || input == "done" || input == "d") {
         break;
      }
      
      for (int i = input.size() - 1; i >= 0; --i) {
         cout << input.at(i);
      }
      
      cout << endl;
      
   }

   return 0;
}
