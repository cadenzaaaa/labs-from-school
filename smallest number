#include <iostream>
using namespace std;

int main() {
   
   int x;
   int y;
   int z;
   
   cin >> x;
   cin >> y;
   cin >> z;

   if ((x < y) && (x < z)) {
      cout << x << endl;
   }
   else if ((y < x) && (y < z)) {
      cout << y << endl;
   }
   else if ((z < x) && (z < y)) {
      cout << z << endl;
   }
   else if ((x == y) && (x == z)) {
      cout << x << endl;
   }
   return 0;
}
